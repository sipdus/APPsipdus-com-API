        "BLUETOOTH_ADVERTISE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],

      "runtimeVersion": "1.0.0"
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "updates": {
      "url": "https://u.expo.dev/ce2af64a-3f68-4f35-b497-033e591be2d5"
